{% extends "base.html.twig" %}

{% block title %}Generate{% endblock %}
{% block header_title %}{{ block('title') }}{% endblock %}

{% block content %}
    <ul>
        {% for idx, item in tile %}
            <li>
                {{ idx }}
                {{ item.filename }} {{ item.rotation }}
                {{ dump(item.neighborMask) }}
                {{ dump(anchor[idx]) }}
            </li>
        {% endfor %}
    </ul>
        {{ dump(map) }}
{% endblock %}
